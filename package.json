{
  "name": "heartvoice-monitor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:websocket": "tsx src/services/websocket-proxy-server.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:websocket\"",
    "build": "next build --turbopack",
    "start": "next start",
    "start:websocket": "node dist/services/websocket-proxy-server.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:websocket\"",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:comprehensive": "node run-comprehensive-test.js",
    "test:comprehensive:debug": "TEST_DEBUG=1 node run-comprehensive-test.js",
    "test:stagehand": "playwright test tests/stagehand-e2e.spec.ts --headed",
    "test:stagehand:debug": "playwright test tests/stagehand-e2e.spec.ts --headed --debug",
    "test:voice": "playwright test tests/voice-call-focused.spec.ts --project=stagehand --headed",
    "test:voice:continuous": "node run-voice-tests-continuous.js",
    "test:env": "playwright test tests/environment-check.spec.ts --project=chromium --headed",
    "test:openai": "tsx scripts/test-openai-integration.ts",
    "lint": "eslint"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.15.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@shadcn/ui": "^0.0.4",
    "@types/twilio": "^3.19.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "elevenlabs": "^1.59.0",
    "lucide-react": "^0.543.0",
    "next": "15.5.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "twilio": "^5.9.0",
    "ws": "^8.14.2",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@browserbasehq/stagehand": "^2.3.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.5.10",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "shadcn": "^3.2.1",
    "tailwindcss": "^4",
    "tsx": "^4.7.0",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}